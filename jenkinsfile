pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main',
                    url: 'https://github.com/Sakthi0025/day6-html-app.git',
                    credentialsId: 'github-credentials'
            }
        }
        stage('Build') {
            steps {
                echo "Building the project..."
            }
        }
    }
}
